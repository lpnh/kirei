---
source: tests/snapshots/css.rs
description: "<style>\n{% for breakpoint in breakpoints %}\n    @media (min-width: {{ breakpoint.width }}px) {\n        {% for col in 1..=12 %}\n            .{{ breakpoint.name }}-col-{{ col }} {\n                width: {{ col * 100 / 12 }}%;\n            }\n        {% endfor %}\n    }\n{% endfor %}\n</style>\n"
---
<style>
    {% for breakpoint in breakpoints %}
        @media (min-width: {{ breakpoint.width }}px) {
        {% for col in 1..=12 %}
            .{{ breakpoint.name }}-col-{{ col }} {
                            width: {{ col * 100 / 12 }}%;
                        }
        {% endfor %}
        }
    {% endfor %}
</style>
