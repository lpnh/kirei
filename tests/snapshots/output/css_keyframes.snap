---
source: tests/snapshots/css.rs
description: "<style>\n@keyframes {{ animation_name }} {\n    from {\n        opacity: 0;\n        transform: translateY({{ start_offset }}px);\n    }\n    to {\n        opacity: 1;\n        transform: translateY(0);\n    }\n}\n\n.animated {\n    animation: {{ animation_name }} {{ duration }}s ease-out;\n}\n\n{% for i in 1..=3 %}\n@keyframes fade-{{ i }} {\n    from { opacity: 0; }\n    to { opacity: {{ i * 0.33 }}; }\n}\n{% endfor %}\n</style>\n"
---
<style>
    @keyframes {{ animation_name }} {
        from {
            opacity: 0;
            transform: translateY({{ start_offset }}px);
        }
        to {
            opacity: 1;
            transform: translateY(0);
        }
    }

    .animated {
        animation: {{ animation_name }} {{ duration }}s ease-out;
    }

    {% for i in 1..=3 %}
    @keyframes fade-{{ i }} {
        from { opacity: 0; }
        to { opacity: {{ i * 0.33 }}; }
    }
    {% endfor %}
</style>
